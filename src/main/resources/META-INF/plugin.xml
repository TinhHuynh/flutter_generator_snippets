<idea-plugin>
    <id>io.github.tbm98.flutter_generator_snippets</id>
    <name>Flutter snippet for generator tool</name>
    <vendor email="tbm98.dev@gmail.com" url="https://tbm98.dev">tbm98</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="/liveTemplates/Flutter_generator_snippets.xml"/>
        <liveTemplateContext implementation="com.github.tbm98.fluttergeneratorsnippets.FlutterContext" contextId="FLUTTER_GENERATOR_SNIPPETS"/>
    </extensions>

</idea-plugin>
